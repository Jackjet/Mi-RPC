<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:insist="http://www.317hu.com/insist" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       		http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       		http://www.317hu.com/insist insist.xsd"
       default-autowire="byName">

    <bean id="userService" class="org.ahstu.mi.test.service.impl.UserServiceImpl"/>

    <bean id="tradeService" class="org.ahstu.mi.test.service.impl.TradeServiceImpl"/>

    <bean id="courseCenter" class="org.ahstu.mi.test.service.impl.CourseCenterServiceImpl"/>

    <insist:provider id="userServiceInsist"
                     interface="org.ahstu.mi.test.service.UserService"
                     version="1.0.0.daily"
                     group="user"
                     ref="userService"
                     serviceDesc="user center interface"></insist:provider>

    <insist:provider id="tradeServiceInsist"
                     interface="org.ahstu.mi.test.service.TradeService"
                     version="1.0.0.daily"
                     group="trade"
                     ref="tradeService"
                     serviceDesc="trade center interface"></insist:provider>

    <insist:provider id="courseCenterInsist" interface="org.ahstu.mi.test.service.CourseCenterService"
                     version="1.0.0.daily" group="course" ref="courseCenter"
                     serviceDesc="course center interface"></insist:provider>


</beans>